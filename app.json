    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/udacifitness.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "org.udacifitness.exp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
