{
  "expo": {
    "name": "UdaciFitness",
    "slug": "udacifitness",
    "description": "The Best triathlon tracker ever",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/udacifitness.png",
    "splash": {
      "image": "./assets/udacifitness.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/udacifitness.png"
